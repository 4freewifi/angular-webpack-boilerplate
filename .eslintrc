{
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": [ 2, "always-multiline" ],
    "no-console": 0,
    "indent": [ 2, 2 ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "always" ]
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
}
